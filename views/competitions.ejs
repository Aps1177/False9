<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/head')-%>
    <title>Document</title>
</head>
<body>
  <%- include('partials/navbar')-%>


  <div class="container-fluid">
  <div class="row">
   <% for(let i=0; i<noOfComp; i++){ %>
   <%    if(displayData.competitions[i].plan === "TIER_ONE"){ %>
          <div class="col-sm-4">
        <div class="card w-50">
          <div class="card-body">
            <h5 class="card-title"><%= displayData.competitions[i].name %></h5>
            <p class="card-text"><%= displayData.competitions[i].area.name %></p>
            <a href="competitions/<%= displayData.competitions[i].code %>" role="button" class="btn btn-primary" id=" <%= displayData.competitions[i].code %> " >View Teams  </a>
          </div>
        </div>
      </div>       
        <% } %>
  <% } %>
</div>
</div>
</body>
</html>